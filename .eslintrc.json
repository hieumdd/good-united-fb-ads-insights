{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-console": 0,
        "no-unused-variable": 0,
        "no-unused-vars": 0,
        "no-else-return": 0,
        "dot-notation": 0
    }
}
